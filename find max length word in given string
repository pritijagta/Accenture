
class Main {
    
    public static String longest_String(String s)
    {
        String s1[]=s.split(" ");
        String result=" ";
        for(int i=0;i<s1.length;i++)
        {
            if(s1[i].length()>result.length())
            {
               result=s1[i];
            }
        }
      
        return result;
        
    }
    public static void main(String[] args) {
        
        System.out.print(longest_String("yes nofsdfsdfs number"));
    }
}
